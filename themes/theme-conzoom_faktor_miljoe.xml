<?xml version="1.0" encoding="ISO-8859-1"?>
<theme>
    <clientlayers>
        <clientlayer>
            <singletile>true</singletile>
            <datasource>wms_miljoefaktor</datasource>
        </clientlayer>
    </clientlayers>
    <!--   
    ===============================================================
    Conzoom public, Syddjurs Kommune
    ===============================================================
	Conzoom, miljøfaktor
    =============================================================== 
    -->
    <cbinfo-metadata>
        <param name="metadata.text"><![CDATA[Geomatic public data. ]]></param>
        <param name="copyright-text"><![CDATA[© Geomatic]]></param>
        <param name="copyright.url"><![CDATA[http://geomatic.dk/]]></param>
        <param name="metadata.url"><![CDATA[http://issuu.com/geomatic/docs/geomatic_dokumentation_af_data/12]]></param>
    </cbinfo-metadata>
    <layer name="theme-conzoom_miljoefaktor" type="RASTER">[datasource:wms_miljoefaktor.mapfile-datasource]
        <transparency>100</transparency>
    </layer>
    <layer name="dummy" type="POLYGON">
        <class>
            <name>1: Ikke miljøbevidste i ders forbrug og mener vækst er vigtigere</name>
            <style>
                <color>150 150 150</color>
            </style>
        </class>
        <class>
            <name>2: Lav interesse for miljø og tænker ikke over det i deres forbrug</name>
            <style>
                <color>143 175 199</color>
            </style>
        </class>
        <class>
            <name>3: Lav interesse for miljø, dog i nogen grad imod at vægte vækst højere </name>
            <style>
                <color>127 175 227</color>
            </style>
        </class>
        <class>
            <name>4:  Lav interesse for miljø, dog i nogen grad bevidste som forbrugere</name>
            <style>
                <color>192 214 240</color>
            </style>
        </class>
        <class>
            <name>5: Vækst vigtigere end miljø, dog både interesse for og bevidsthed om som forbrugere</name>
            <style>
                <color>191 222 191</color>
            </style>
        </class>
        <class>
            <name>6: Vækst ikke vigtigere end miljø og i nogen grad miljøbevidst som forbrugere</name>
            <style>
                <color>145 201 145</color>
            </style>
        </class>
        <class>
            <name>7: Vækst er vigtigst, men miljøbevidste som forbrugere</name>
            <style>
                <color>225 230 128</color>
            </style>
        </class>
        <class>
            <name>8: Miljøinteresserede og -bevidste i forbrug og holdning</name>
            <style>
                <color>247 187 143</color>
            </style>
        </class>
        <class>
            <name>9: Meget miljøinteresserede og -bevidste i forbrug og holdning</name>
            <style>
                <color>247 146 146</color>
            </style>
        </class>
        <class>
            <name>10: Mest miljøinteresserede og -bevidste i forbrug og holdning</name>
            <style>
                <color>214 143 143</color>
            </style>
        </class>
    </layer>
</theme>
